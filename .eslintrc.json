{
    "env": {
        "node":true,
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
      "settings": {
    "import/core-modules": ["express-rate-limit","celebrate", "helmet"]

  },
    "rules": {
    "func-names": ["off"],
    "no-underscore-dangle": ["error", { "allow": ["_id"]}],
      "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
}
